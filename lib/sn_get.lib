
#---Get SN---
#. ./lib/loc_var.lib
SN_TMP=$TMP_LOC/sn.tmp
rm -f $SN_TMP
read -p "Please input SN: " SN 
echo $SN-$(date '+%Y%m%d%H%M%S') > $SN_TMP
SN_LOG=$(<$SN_TMP)
#echo Model: $0 >> $LOG_LOC/$SN_LOG.log
cat $TMP_LOC/model.tmp >> $LOG_LOC/$SN_LOG.log   
echo Serial Number: $SN >> $LOG_LOC/$SN_LOG.log	

