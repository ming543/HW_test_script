
#---Get SN---
sn_get(){
        SN_TMP=/run/initramfs/memory/data/HW_test_script/tmp/sn.tmp
        rm -f $SN_TMP
	read -p "請輸入序號: " SN 
	echo $SN-$(date '+%Y%m%d%H%M%S') > $SN_TMP
	SN_LOG=$(<$SN_TMP)
        cat $TEMP_LOG/model.tmp >> $USB_LOG/$SN_LOG.log	
        echo Serial Number: $SN >> $USB_LOG/$SN_LOG.log	
}

